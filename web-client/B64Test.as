package 
{
	// import ->
	import flash.display.MovieClip;
	import flash.display.Loader;
	import flash.util.trace;
	import flash.util.ByteArray;
	import formatter.Base64;
	// <- import
	
	// B64Test ->
	public class B64Test extends MovieClip 
	{
		public function B64Test() 
		{
			// an example of encoding/decoding strings
			var pwStr:String= "username : password";
			var bArr:ByteArray=new ByteArray();
			bArr.writeUTFBytes(pwStr);
			var pwStrEncoded:String=Base64.Encode(bArr, true);
			var pwStrDecoded:ByteArray=Base64.Decode(pwStrEncoded);
			
			trace("Base64Encoded: "+pwStrEncoded+"\nBase64Decoded: "+pwStrDecoded.toString());
			
			// an example of decoding a B64 encoded binary (GIF in this case) and displaying it
			// the flashforum.de headerlogo encoded in B64
			var imgB64Str:String=""; 
			var ld:Loader=new Loader();
			// have to love loadBytes in as3 :)
			ld.loadBytes( Base64.Decode(imgB64Str) );
			addChild(ld);
		}
	}
	// <- B64Test
}
